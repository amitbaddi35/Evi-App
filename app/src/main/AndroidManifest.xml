<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smsoft.evischoolmanagementapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Timetable"></activity>
        <activity android:name=".Exam" />
        <activity android:name=".Feedback" />
        <activity android:name=".Attendance" />
        <activity android:name=".Events" />
        <activity android:name=".Notifications" />
        <activity android:name=".FeesDetails" />
        <activity android:name=".SchoolDashBoard" />
        <activity android:name=".schoolLogin" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>